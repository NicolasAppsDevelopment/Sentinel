<a class="quiz-card" href="{{ path('app_couples_view', {'id': couple.id}) }}">
    <div>
        <div class="quiz-card-image">
            <div class="image-cover"></div>
        </div>
        <div class="quiz-card-info">
            <h3 class="title">{{ couple.title }}</h3>
            <div class="row g0-5">
                {% if couple.enabled == 1 %}
                    <img class="tag small" src="{{ asset('icons/enabled-camera.svg') }}">
                {% else %}
                    {# <img class="tag small" src="{{ asset('icons/disabled-camera.svg') }}"> #}
                    <div class="tag small">
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                            <path fill="currentColor" d="m22 17.5l-4-4v1.675l-2-2V6H8.825l-2-2H16q.825 0 1.413.588T18 6v4.5l4-4zm-1.45 5.85L.65 3.45l1.4-1.4l19.9 19.9zM4 4l2 2H4v12h12v-2l2 2q0 .825-.587 1.413T16 20H4q-.825 0-1.412-.587T2 18V6q0-.825.588-1.412T4 4m2 12v-.55q0-1.1 1.1-1.775T10 13t2.9.675T14 15.45V16z"/>
                        </svg>
                    </div>
                {% endif %}

                {% if wifi == "good" %}
                    <div class="tag small"> 
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                            <path fill="currentColor" d="m6.35 15.35l-2.1-2.15q1.55-1.55 3.55-2.375T12 10t4.213.838t3.537 2.412l-2.1 2.1q-1.125-1.125-2.588-1.737T12 13t-3.062.613T6.35 15.35M2.1 11.1L0 9q2.375-2.425 5.488-3.713T12 4t6.513 1.288T24 9l-2.1 2.1q-1.975-1.975-4.538-3.037T12 7T6.637 8.063T2.1 11.1M12 21l-3.525-3.55q.7-.7 1.613-1.075T12 16t1.913.375t1.612 1.075z"/>
                        </svg>
                    </div>

                {% elseif wifi == "mid" %}
                    <div class="tag small"> 
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                            <path fill="currentColor" d="m6.35 15.35l-2.1-2.15q1.55-1.55 3.55-2.375T12 10t4.213.838t3.537 2.412l-2.1 2.1q-1.125-1.125-2.588-1.737T12 13t-3.062.613T6.35 15.35M12 21l-3.525-3.55q.7-.7 1.613-1.075T12 16t1.913.375t1.612 1.075z"/>
                        </svg>
                    </div>
                {% elseif wifi == "low" %}
                    <div class="tag small"> 
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                            <path fill="currentColor" d="m12 21l-3.525-3.55q.7-.7 1.613-1.075T12 16t1.913.375t1.612 1.075z"/>
                        </svg>
                    </div>
                {% else %}
                    <div class="tag small"> 
                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24">
                            <path fill="currentColor" d="m16.9 20.5l-1.4-1.4l2.1-2.1l-2.1-2.1l1.4-1.4l2.1 2.1l2.1-2.1l1.425 1.4l-2.1 2.1l2.075 2.1l-1.4 1.4l-2.1-2.075zM12 21L0 9q2.375-2.425 5.488-3.713T12 4t6.513 1.288T24 9l-3.45 3.425q-.35-.35-.7-.712t-.7-.713l1.95-1.95q-1.975-1.5-4.3-2.275T12 6t-4.8.775T2.9 9.05l9.1 9.1l1-1l.713.713l.712.712zm0-8.925"/>
                        </svg>
                    </div>
                {% endif %}
                {# <p class="date tag small">
                    <i class="fa-regular fa-calendar"></i> {{ couple.association_date }}
                </p> #}
            </div>
        </div>
    </div>
</a>